{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "provider": "jsdelivr",
      "library": "toastify-js@1.6.2",
      "destination": "wwwroot/lib/toastify-js/",
      "files": [
        "src/toastify.min.css",
        "src/toastify.min.js",
        "src/toastify.js",
        "src/toastify.css"
      ]
    }
  ]
}